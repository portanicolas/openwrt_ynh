{
  "name": "OpenWRT",
  "id": "openwrt",
  "description": {
    "en": "Access to OpenWRT",
    "fr": "Accès à OpenWRT "
  },
  "developer": {
    "name": "lunarok",
    "email": "cedric@wayang.fr",
    "url": "http://www.wayang.fr"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for OpenWRT"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for OpenWRT"
        },
        "example": "/",
        "default": "/"
      },
      {
        "name": "server",
        "ask": {
          "en": "Choose IP where OpenWRT is installed"
        },
        "example": "127.0.0.1",
        "default": "127.0.0.1"
      }
    ]
  }
}
